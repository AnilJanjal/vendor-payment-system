{
  "version": 2,
  "builds": [
    { "src": "public/taskpane.html", "use": "@vercel/static" },
    { "src": "public/commands.html", "use": "@vercel/static" },
    { "src": "public/manifest.xml", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/taskpane.html", "dest": "public/taskpane.html" },
    { "src": "/commands.html", "dest": "public/commands.html" },
    { "src": "/manifest.xml", "dest": "public/manifest.xml" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
